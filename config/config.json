{
	"network": {
		"instance": "foo.bar.request.handler",
		"debug": true,
		
		"host": "localhost",
		"port": 8001,
		"secure": {
			"cert_path": "keys/cert.crt",
			"cert_key_path": "keys/cert.key",
			"cert_passwd": "1234"
		}
	},
	"files": [
		{
			"types": ".html",
			"alias": "upload",
			"location": "html/upload_file.html",
			"cache": true
		},
		{
			"types": ".html",
			"alias": "show",
			"location": "html/show_file.html",
			"cache": true
		},
		{
			"types": ".jpg,.jpeg",
			"location": "html/upload.jpg",
			"cache": false
		}
	]
}