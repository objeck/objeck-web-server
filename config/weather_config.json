{
	"network": {
		"instance": "RequestHandler",
		"port": 8081,
		"debug": false,
		"secure": {
			"cert_path": "keys/cert.crt",
			"cert_key_path": "keys/cert.key",
			"cert_passwd": "1234"
		}
	},
	"files": {
		"common": {
			"base_dir": null
		},
		"default": {
			"groups": ".html",
			"alias": "index.html",
			"mime-type": "text/html",
			"location": "html/index.html",
			"cache": true
		},
		"items": [
			{
				"groups": ".html",
				"alias": "index.html",
				"mime-type": "text/html",
				"location": "html/weather.html",
				"cache": true
			},
			{
				"groups": ".css",
				"alias": "styles.css",
				"mime-type": "text/css",
				"location": "html/styles.css",
				"cache": true
			},
			{
				"groups": ".png",
				"alias": "images/day.png",
				"mime-type": "image/png",
				"location": "html/images/day.png",
				"cache": true
			},
			{
				"groups": ".png",
				"alias": "images/night.png",
				"mime-type": "image/png",
				"location": "html/images/night.png",
				"cache": true
			},
			{
				"groups": ".png",
				"alias": "images/now.png",
				"mime-type": "image/png",
				"location": "html/images/now.png",
				"cache": true
			}
		]
	}
}