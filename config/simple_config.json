{
	"network": {
		"instance": "RequestHandler",
		"port": 8081,
		"debug": true,
		"secure": {
			"cert_path": "keys/cert.crt",
			"cert_key_path": "keys/cert.key",
			"cert_passwd": "1234"
		}
	},
	"files": {
		"common": {
			"base_dir": null,
			"default": {
				"alias": "index.html",
				"mime-type": "text/html",
				"location": "html/index.html",
				"cache": true
			}
		},
		"types": [
			{
				"group": ".html",
				"files": [
					{
						"alias": "index.html",
						"mime-type": "text/html",
						"location": "html/index.html",
						"cache": true
					},
					{
						"alias": "response.html",
						"mime-type": "text/html",
						"location": "html/response.html",
						"cache": true
					}
				]
			},
			{
				"group": ".jpg,.jpeg",
				"files": [
					{
						"alias": "upload.jpg",
						"location": "html/upload.jpg",
						"cache": false
					}
				]
			}
		]
	}
}