{
	"network": {
		"instance": "RequestHandler",
		"port": 8081,
		"debug": true,
		"secure": {
			"cert_path": "keys/cert.crt",
			"cert_key_path": "keys/cert.key",
			"cert_passwd": "1234"
		}
	},
	"files": {
		"common": {
			"base_dir": null
		},
		"items": [
			{
				"groups": ".html",
				"alias": "index.html",
				"mime-type": "text/html",
				"location": "html/index.html",
				"cache": true
			},
			{
				"groups": ".html",
				"alias": "upload",
				"location": "html/upload_file.html",
				"cache": true
			},
			{
				"groups": ".html",
				"alias": "show",
				"location": "html/show_file.html",
				"cache": true
			},
			{
				"groups": ".jpg,.jpeg",
				"alias": "upload.jpg",
				"location": "html/upload.jpg",
				"cache": false
			}
		]
	}
}